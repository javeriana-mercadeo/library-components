<section class="container" id="student-slider">
    <div class="slider-container">
        <h2
            class="title title-lg slider-title"
            data-lfr-editable-id="title"
            data-lfr-editable-type="rich-text">
            Estudiantes
        </h2>
        <div class="slider-content" id="student-slider-content">
            <div class="carousel-controlsE">
                <button
                    class="carousel-controlE prev"
                    id="student-slider-prev"
                    aria-label="Anterior estudiante"
                    type="button">
                    <i class="ph ph-arrow-circle-left"></i>
                </button>
                <button
                    class="carousel-controlE next"
                    id="student-slider-next"
                    aria-label="Siguiente estudiante"
                    type="button">
                    <i class="ph ph-arrow-circle-right"></i>
                </button>
            </div>
          <lfr-widget-web-content id="widget1"></lfr-widget-web-content>
        </div>
        <div class="slider-dots" id="student-slider-dots">
            <span
                class="dot"
                id="student-slider-dot-0"
                data-slide="0"
                role="button"
                tabindex="0"
                aria-label="Ir al estudiante 1"></span>
            <span
                class="dot"
                id="student-slider-dot-1"
                data-slide="1"
                role="button"
                tabindex="0"
                aria-label="Ir al estudiante 2"></span>
            <span
                class="dot"
                id="student-slider-dot-2"
                data-slide="2"
                role="button"
                tabindex="0"
                aria-label="Ir al estudiante 3"></span>
            <span
                class="dot"
                id="student-slider-dot-3"
                data-slide="3"
                role="button"
                tabindex="0"
                aria-label="Ir al estudiante 4"></span>
            <span
                class="dot"
                id="student-slider-dot-4"
                data-slide="4"
                role="button"
                tabindex="0"
                aria-label="Ir al estudiante 5"></span>
        </div>
    </div>
</section>